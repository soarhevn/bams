            <xml-property name="queryText"><![CDATA[SELECT idMaster, 
CAST(TRIM(LEADING '0' FROM (DATE_SUB(transDate, INTERVAL 1911 YEAR))) AS CHAR) AS transDate, 
CONCAT(aN.accountName, ' - ', acT.atName) AS accountName, 
trans.debit, trans.credit, notes
FROM 
( /* get transactions out and match to transMaster with both debit and credit */
SELECT DISTINCT idMaster, transDate, notes, accountID, debit, credit
FROM transactionsMaster
LEFT JOIN transactions t USING (idMaster)
WHERE t.accountID = ?
AND transDate BETWEEN REPLACE(?, (SUBSTRING_INDEX(?, '-', 1)), ((SUBSTRING_INDEX(?, '-', 1)) + 1911)) 
AND REPLACE(?, (SUBSTRING_INDEX(?, '-', 1)), ((SUBSTRING_INDEX(?, '-', 1)) + 1911)) ) trans 
  /* end tranactions section */
  /* start joins for account names + account types */
LEFT JOIN accountNames aN ON trans.accountID = aN.accountID
LEFT JOIN accountType acT ON aN.accountType = acT.acctTypeID
ORDER BY transdate, idMaster ASC]]></xml-property>
