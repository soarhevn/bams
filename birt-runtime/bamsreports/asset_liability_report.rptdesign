            <xml-property name="queryText"><![CDATA[SELECT idMaster, 
CAST(TRIM(LEADING '0' FROM (DATE_SUB(transDate, INTERVAL 1911 YEAR))) AS CHAR) AS transDate, 
IF(idMaster = -1, '早先平衡', CONCAT(aN.accountName, ' - ', acT.atName)) AS accountName, 
trans.debit, trans.credit, notes
FROM 
( /* get transactions out and match to transMaster for single line with both debit and credit */

( /* get total of debits and credit for previous trans */
SELECT -1 AS idMaster, NULL AS transDate, NULL AS accountID, 
IFNULL(SUM(debit),0) AS debit, IFNULL(SUM(credit),0) AS credit,
CONCAT('早先總額 到', SUBSTRING((CONCAT(r.yearR, '-', r.monthR, '-01') - INTERVAL 1 DAY),3)) AS notes
FROM transactionsMaster
LEFT JOIN transactions USING (idMaster)
LEFT JOIN accountGroupXref gX ON (accountID = gX.accName_id)
LEFT JOIN reporting r ON (gX.accGrp_id = r.groupID)
WHERE r.id = ?
AND transDate < CONCAT(r.yearR+1911, '-', r.monthR, '-01')
GROUP BY idMaster
)
UNION
( /* get transactions out and match to transMaster with both debit and credit */
SELECT DISTINCT idMaster, transDate, t2.accountID, t1.debit, t1.credit, notes
FROM transactionsMaster
LEFT JOIN transactions t1 USING (idMaster)
LEFT JOIN transactions t2 USING (idMaster)
LEFT JOIN accountGroupXref gX ON (t1.accountID = gX.accName_id)
LEFT JOIN reporting r ON (gX.accGrp_id = r.groupID)
WHERE r.id = ?
AND t1.accountID != t2.accountID
AND YEAR(transDate) = (r.yearR + 1911)
AND MONTH(transDate) = r.MonthR 
)) trans 
  /* end tranactions section */
  /* start joins for account names + account types */
LEFT JOIN accountNames aN ON trans.accountID = aN.accountID
LEFT JOIN accountType acT ON aN.accountType = acT.acctTypeID
ORDER BY transdate, idMaster ASC]]></xml-property>
